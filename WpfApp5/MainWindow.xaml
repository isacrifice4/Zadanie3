        <TextBox HorizontalAlignment="Left" Height="23" Margin="10,101,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120" Name="textBox3" Text="3"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="185,9,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <ListBox HorizontalAlignment="Left" Height="100" Margin="185,49,0,0" VerticalAlignment="Top" Width="280" Name="listBox1"/>

    </Grid>
